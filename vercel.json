{
  "headers": [
    {
      "source": "/(.*.js)",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    }
  ],
  "routes": [
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/src/(.*)",
      "dest": "/js/$1",
      "check": true
    }
  ]
} 